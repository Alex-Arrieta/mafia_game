<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://test.org/onto_Alice.owl"
         xmlns="http://test.org/onto_Alice.owl#"
         xmlns:onto="http://test.org/onto_Dana.owl#">

<owl:Ontology rdf:about="http://test.org/onto_Alice.owl"/>

<owl:ObjectProperty rdf:about="#is_playing_in">
  <rdfs:domain rdf:resource="#Player"/>
  <rdfs:range rdf:resource="#Mafia_Game_Knowledge"/>
  <owl:inverseOf rdf:resource="#has_player"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_player">
  <rdfs:domain rdf:resource="#Mafia_Game_Knowledge"/>
  <rdfs:range rdf:resource="#Player"/>
  <owl:inverseOf rdf:resource="#is_playing_in"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#alive">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Player"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#role">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Player"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#potentialRole">
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Mafia_Game_Knowledge">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Player">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:NamedIndividual rdf:about="#my_game_Alice">
  <rdf:type rdf:resource="#Mafia_Game_Knowledge"/>
  <onto:has_player rdf:resource="#player_Alice"/>
  <onto:has_player rdf:resource="#player_Bob"/>
  <onto:has_player rdf:resource="#player_Charlie"/>
  <onto:has_player rdf:resource="#player_Dana"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#player_Alice">
  <rdf:type rdf:resource="#Player"/>
  <onto:alive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</onto:alive>
  <onto:role rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mafia</onto:role>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#player_Bob">
  <rdf:type rdf:resource="#Player"/>
  <onto:role rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unknown</onto:role>
  <onto:alive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</onto:alive>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#player_Charlie">
  <rdf:type rdf:resource="#Player"/>
  <onto:role rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unknown</onto:role>
  <onto:alive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</onto:alive>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#player_Dana">
  <rdf:type rdf:resource="#Player"/>
  <onto:alive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</onto:alive>
  <onto:role rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unknown</onto:role>
</owl:NamedIndividual>


</rdf:RDF>
